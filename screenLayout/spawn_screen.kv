<SpawnScreen>:
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'assets/img/spawn_splash.png'

    
    FloatLayout:
        Button:
            size_hint: None,None
            size: dp(60), dp(60)
            pos: dp(12), dp(523)
            background_color: 0,0,0,0
            on_press: root.manager.current = 'main'

            Image:
                source: 'assets/img/button_return.png'
                center_x: self.parent.center_x
                center_y: self.parent.center_y